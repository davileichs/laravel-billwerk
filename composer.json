{
    "name": "tda/laravel-billwerk",
    "description": "Laravel package for Billwerk API",
    "keywords": [
        "tda",
        "laravel",
        "billwerk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "DL",
            "email": "davileichs@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tda\\LaravelBillwerk\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tda\\LaravelBillwerk\\BillwerkServiceProvider"
            ],
            "aliases": {
                "Billwerk": "Tda\\LaravelBillwerk\\Facades\\Billwerk"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
